let add(x: int, y: int): int { x + y }

// good
add(2, 3);

// bad
add("2", "3");

let toString(n: int): string {
    n
}

// Can only use booleans with logic operators
true && 1;
"false" || false;
2 && "4";

// Invalid left-hand side in assignment
true = false;
1 = 2;
"string" = "other string";
{ 10 } = { 20 };